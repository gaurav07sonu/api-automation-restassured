<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >
<suite name="Sentieo tests" parallel="classes" thread-count="1">
	<listeners>
		<listener class-name="com.sentieo.listener.TestListener" />
		<listener
			class-name="com.sentieo.listener.AnnotationTransformer" />
		<listener
			class-name="com.epam.reportportal.testng.ReportPortalTestNGListener" />
	</listeners>

	<!-- <test name="finance" preserve-order="true"> <packages> <package name="com.sentieo.finance" 
		/> </packages> </test> <test name="others" preserve-order="true"> <packages> 
		<package name="com.sentieo.*" > <exclude name="com.sentieo.finance"></exclude> 
		</package> </packages> </test> -->

	<test name="notebook" preserve-order="true">
	<classes>
		<class name="com.sentieo.notebook.NotebookApis">
			<methods>
				<exclude name="createThesis" />
				<exclude name="createThesisTemplate" />
				<exclude name="createTabTemplate" />
				<exclude name="deleteThesisTemplate" />
			</methods>
		</class>
	</classes>
</test>
</suite>