<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >
<suite name="Sentieo tests" parallel="methods" thread-count="1">
	<listeners>
		<listener class-name="com.sentieo.listener.TestListener" />
		<listener
			class-name="com.sentieo.listener.AnnotationTransformer" />
		 <listener
			class-name="com.epam.reportportal.testng.ReportPortalTestNGListener" />
	</listeners>

	<test name="notebookPublic" preserve-order="true">
		<classes>
			<class name="com.sentieo.notebookpublicapis.NotebookPublicApis">
				<methods>
					<exclude name="testLargeFileUpload"/>
				</methods>
			</class>
		</classes>
	</test>
</suite>