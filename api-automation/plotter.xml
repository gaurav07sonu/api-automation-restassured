<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >
<suite name="Sentieo tests" parallel="classes" thread-count="1" >
   <listeners>
        <listener class-name="com.sentieo.listener.TestListener"/>
        <listener class-name="com.sentieo.listener.AnnotationTransformer"/> 
   </listeners>
 <test name="others" preserve-order="true">
		<packages>
			<package name="com.sentieo.plotter">
				<exclude name="com.sentieo.plotter.*"></exclude>
			</package>
		</packages>
	</test>
  
	<test name="plotter test">
		<classes>
<!-- 			<class name="com.sentieo.plotter.DocumentData" /> -->
			<class name="com.sentieo.plotter.IncomeStatement" />
			<class name="com.sentieo.plotter.KeyMultiples" />
			<class name="com.sentieo.plotter.LoadGraph" />
			<class name="com.sentieo.plotter.StockData" />
			<class name="com.sentieo.plotter.StockPrice49929"/>
			<class name="com.sentieo.plotter.StockPrice50650"/>
			<class name="com.sentieo.plotter.TestDailySeriesData" />
			<class name="com.sentieo.plotter.WebandSocialData" />
			<class name="com.sentieo.plotter.TestPENotUpdatingBug53404" />
			<class name="com.sentieo.plotter.FetchSavedSeries" />
		</classes>
	</test>

</suite>
