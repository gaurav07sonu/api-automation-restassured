<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >
<suite name="Sentieo tests" parallel="classes" thread-count="1">
	<listeners>
		<listener class-name="com.sentieo.listener.TestListener" />
		<listener
			class-name="com.sentieo.listener.AnnotationTransformer" />
	
	</listeners>

	<test name="sentieo-tests1" preserve-order="true">
		<classes>
			<class name="com.sentieo.watchlistsharing.ShareWatchlistWithCopyPermission" />
		</classes>
	</test>
	<test name="sentieo-tests2" preserve-order="true">
		<classes>
			<class name="com.sentieo.watchlistsharing.ShareWatchlistWithEditPermission" />
		</classes>
	</test>
	<test name="sentieo-tests3" preserve-order="true">
		<classes>
			<class name="com.sentieo.watchlistsharing.ShareWatchlistWithViewPermission" />
		</classes>
	</test>
</suite>
