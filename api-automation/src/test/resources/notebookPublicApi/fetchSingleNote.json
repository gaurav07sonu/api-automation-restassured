{
  "definitions": {},
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "http://example.com/root.json",
  "type": "object",
  "title": "The Root Schema",
  "required": [
    "category",
    "attachments",
    "shared_with_users",
    "title",
    "highlights",
    "created_at",
    "tags",
    "updated_at",
    "active",
    "version",
    "short_url",
    "owner",
    "tickers",
    "type",
    "id",
    "shared_with_groups"
  ],
  "properties": {
    "category": {
      "$id": "#/properties/category",
      "type": "string",
      "title": "The Category Schema",
      "default": "",
      "examples": [
        "General"
      ],
      "pattern": "^(.*)$"
    },
    "attachments": {
      "$id": "#/properties/attachments",
      "type": "array",
      "title": "The Attachments Schema"
    },
    "shared_with_users": {
      "$id": "#/properties/shared_with_users",
      "type": "array",
      "title": "The Shared_with_users Schema",
      "items": {
        "$id": "#/properties/shared_with_users/items",
        "type": "string",
        "title": "The Items Schema",
        "default": "",
        "examples": [
          "schroders.check"
        ],
        "pattern": "^(.*)$"
      }
    },
    "title": {
      "$id": "#/properties/title",
      "type": "string",
      "title": "The Title Schema",
      "default": "",
      "examples": [
        "note.pdf"
      ],
      "pattern": "^(.*)$"
    },
    "highlights": {
      "$id": "#/properties/highlights",
      "type": "array",
      "title": "The Highlights Schema"
    },
    "created_at": {
      "$id": "#/properties/created_at",
      "type": "string",
      "title": "The Created_at Schema",
      "default": "",
      "examples": [
        "2020-01-13T06:45:18"
      ],
      "pattern": "^(.*)$"
    },
    "tags": {
      "$id": "#/properties/tags",
      "type": "array",
      "title": "The Tags Schema",
      "items": {
        "$id": "#/properties/tags/items",
        "type": "string",
        "title": "The Items Schema",
        "default": "",
        "examples": [
          "a"
        ],
        "pattern": "^(.*)$"
      }
    },
    "updated_at": {
      "$id": "#/properties/updated_at",
      "type": "string",
      "title": "The Updated_at Schema",
      "default": "",
      "examples": [
        "2020-01-13T06:45:34"
      ],
      "pattern": "^(.*)$"
    },
    "active": {
      "$id": "#/properties/active",
      "type": "integer",
      "title": "The Active Schema",
      "default": 0,
      "examples": [
        1
      ]
    },
    "version": {
      "$id": "#/properties/version",
      "type": "integer",
      "title": "The Version Schema",
      "default": 0,
      "examples": [
        1
      ]
    },
    "short_url": {
      "$id": "#/properties/short_url",
      "type": "string",
      "title": "The Short_url Schema",
      "default": "",
      "examples": [
        "http://snt.io/9SFBtVRsT"
      ],
      "pattern": "^(.*)$"
    },
    "owner": {
      "$id": "#/properties/owner",
      "type": "string",
      "title": "The Owner Schema",
      "default": "",
      "examples": [
        "schroders.check"
      ],
      "pattern": "^(.*)$"
    },
    "tickers": {
      "$id": "#/properties/tickers",
      "type": "array",
      "title": "The Tickers Schema",
      "items": {
        "$id": "#/properties/tickers/items",
        "type": "string",
        "title": "The Items Schema",
        "default": "",
        "examples": [
          "fb",
          "msft"
        ],
        "pattern": "^(.*)$"
      }
    },
    "type": {
      "$id": "#/properties/type",
      "type": "string",
      "title": "The Type Schema",
      "default": "",
      "examples": [
        "attachment"
      ],
      "pattern": "^(.*)$"
    },
    "id": {
      "$id": "#/properties/id",
      "type": "string",
      "title": "The Id Schema",
      "default": "",
      "examples": [
        "5e1c11fee8f6c23c9595d1a5"
      ],
      "pattern": "^(.*)$"
    },
    "shared_with_groups": {
      "$id": "#/properties/shared_with_groups",
      "type": "array",
      "title": "The Shared_with_groups Schema",
      "items": {
        "$id": "#/properties/shared_with_groups/items",
        "type": "string",
        "title": "The Items Schema",
        "default": "",
        "examples": [
          "schroderscheck"
        ],
        "pattern": "^(.*)$"
      }
    }
  }
}