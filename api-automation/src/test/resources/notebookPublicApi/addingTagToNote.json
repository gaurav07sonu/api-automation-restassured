{
  "definitions": {},
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "http://example.com/root.json",
  "type": "object",
  "title": "The Root Schema",
  "required": [
    "parent_note_id",
    "tags"
  ],
  "properties": {
    "parent_note_id": {
      "$id": "#/properties/parent_note_id",
      "type": "string",
      "title": "The Parent_note_id Schema",
      "default": "",
      "examples": [
        "5e202cf3e8f6c21b664386d1"
      ],
      "pattern": "^(.*)$"
    },
    "tags": {
      "$id": "#/properties/tags",
      "type": "array",
      "title": "The Tags Schema",
      "items": {
        "$id": "#/properties/tags/items",
        "type": "string",
        "title": "The Items Schema",
        "default": "",
        "examples": [
          "mynote"
        ],
        "pattern": "^(.*)$"
      }
    }
  }
}