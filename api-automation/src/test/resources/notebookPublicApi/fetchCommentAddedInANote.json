{
  "definitions": {},
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "http://example.com/root.json",
  "type": "object",
  "title": "The Root Schema",
  "required": [
    "offset",
    "limit",
    "entries"
  ],
  "properties": {
    "offset": {
      "$id": "#/properties/offset",
      "type": "integer",
      "title": "The Offset Schema",
      "default": 0,
      "examples": [
        0
      ]
    },
    "limit": {
      "$id": "#/properties/limit",
      "type": "integer",
      "title": "The Limit Schema",
      "default": 0,
      "examples": [
        1
      ]
    },
    "entries": {
      "$id": "#/properties/entries",
      "type": "array",
      "title": "The Entries Schema",
      "items": {
        "$id": "#/properties/entries/items",
        "type": "object",
        "title": "The Items Schema",
        "required": [
          "comment",
          "status",
          "is_edited",
          "text",
          "comment_id",
          "updated_at",
          "reference_id",
          "last_updated_at",
          "description_code",
          "created_by",
          "source",
          "location",
          "created_at"
        ],
        "properties": {
          "comment": {
            "$id": "#/properties/entries/items/properties/comment",
            "type": "string",
            "title": "The Comment Schema",
            "default": "",
            "examples": [
              "asfASJF"
            ],
            "pattern": "^(.*)$"
          },
          "status": {
            "$id": "#/properties/entries/items/properties/status",
            "type": "integer",
            "title": "The Status Schema",
            "default": 0,
            "examples": [
              1
            ]
          },
          "is_edited": {
            "$id": "#/properties/entries/items/properties/is_edited",
            "type": "boolean",
            "title": "The Is_edited Schema",
            "default": false,
            "examples": [
              false
            ]
          },
          "text": {
            "$id": "#/properties/entries/items/properties/text",
            "type": "string",
            "title": "The Text Schema",
            "default": "",
            "examples": [
              "asfASJF"
            ],
            "pattern": "^(.*)$"
          },
          "comment_id": {
            "$id": "#/properties/entries/items/properties/comment_id",
            "type": "string",
            "title": "The Comment_id Schema",
            "default": "",
            "examples": [
              "5e1c11a2e8f6c262915e7ca4"
            ],
            "pattern": "^(.*)$"
          },
          "updated_at": {
            "$id": "#/properties/entries/items/properties/updated_at",
            "type": "integer",
            "title": "The Updated_at Schema",
            "default": 0,
            "examples": [
              1578897826000
            ]
          },
          "reference_id": {
            "$id": "#/properties/entries/items/properties/reference_id",
            "type": "string",
            "title": "The Reference_id Schema",
            "default": "",
            "examples": [
              "5e1c0f6be8f6c262915e7c78"
            ],
            "pattern": "^(.*)$"
          },
          "last_updated_at": {
            "$id": "#/properties/entries/items/properties/last_updated_at",
            "type": "integer",
            "title": "The Last_updated_at Schema",
            "default": 0,
            "examples": [
              1578897826000
            ]
          },
          "description_code": {
            "$id": "#/properties/entries/items/properties/description_code",
            "type": "integer",
            "title": "The Description_code Schema",
            "default": 0,
            "examples": [
              0
            ]
          },
          "created_by": {
            "$id": "#/properties/entries/items/properties/created_by",
            "type": "string",
            "title": "The Created_by Schema",
            "default": "",
            "examples": [
              "schroders.check"
            ],
            "pattern": "^(.*)$"
          },
          "source": {
            "$id": "#/properties/entries/items/properties/source",
            "type": "string",
            "title": "The Source Schema",
            "default": "",
            "examples": [
              "note"
            ],
            "pattern": "^(.*)$"
          },
          "location": {
            "$id": "#/properties/entries/items/properties/location",
            "type": "string",
            "title": "The Location Schema",
            "default": "",
            "examples": [
              ""
            ],
            "pattern": "^(.*)$"
          },
          "created_at": {
            "$id": "#/properties/entries/items/properties/created_at",
            "type": "integer",
            "title": "The Created_at Schema",
            "default": 0,
            "examples": [
              1578897826000
            ]
          }
        }
      }
    }
  }
}